
#include "version.hpp"

std::string getVersionString()
{
	return "@PlotSplit_VERSION@";
}

unsigned int getMajorVersion()
{
	return @PlotSplit_VERSION_MAJOR@ +0;
}

unsigned int getMinorVersion()
{
	return @PlotSplit_VERSION_MINOR@ +0;
}

unsigned int getPatchVersion()
{
	return @PlotSplit_VERSION_PATCH@ +0;
}

unsigned int getTweakVersion()
{
	return @PlotSplit_VERSION_TWEAK@ +0;
}
